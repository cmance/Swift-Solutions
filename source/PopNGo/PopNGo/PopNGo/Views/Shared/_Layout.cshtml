<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PopNGo</title>
    <link rel="icon" href="favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/explore.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/homeStyles.css" asp-append-version="true" />
    @* Components/partial views *@
    <link rel="stylesheet" href="~/css/components/eventCard.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/components/weatherCard.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/components/eventDetailsModal.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/components/unauthorizedModal.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/components/eventTicketsVenueModals.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/components/bookmarkListCard.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/components/newBookmarkListCard.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/components/placeholderEventCard.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/components/filters.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/PopNGo.styles.css" asp-append-version="true" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Outfit:wght@800&display=swap" rel="stylesheet">
    @await RenderSectionAsync("Styles", required: false)
</head>

<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm custom-navbar border-0 box-shadow mb-3">
            <div class="container px-5">
                <a aria-label="Home Page Link" id="home-logo" class="navbar-brand text-light onhover" asp-area=""
                    asp-controller="Home" asp-action="Index">
                    <svg width="40pt" height="40pt" viewBox="0 0 483.000000 568.000000">
